{
  "name": "@sanderkooger/mcp-server-ragdocs",
  "version": "1.2.0",
  "description": "An MCP server for semantic documentation search and retrieval using vector databases to augment LLM capabilities.",
  "private": false,
  "engines": {
    "npm": ">=10.0.0",
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm",
    "node": ">=20.0.0"
  },
  "type": "module",
  "bin": {
    "@sanderkooger/mcp-server-ragdocs": "./build/index.js"
  },
  "files": [
    "build",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prepare": "husky",
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "test": "echo 'no tests implemented' && exit 0",
    "lint": "echo 'no linter implemented' && exit 0",
    "ci": "npm run build && npm run lint && npm run test",
    "watch": "tsc --watch",
    "start": "node build/index.js",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js",
    "clean": "rimraf node_modules/ build/ queue.txt"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "rag",
    "documentation",
    "vector-database",
    "qdrant",
    "claude",
    "llm"
  ],
  "author": "sanderkooger",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanderkooger/mcp-server-ragdocs.git"
  },
  "bugs": {
    "url": "https://github.com/sanderkooger/mcp-server-ragdocs/issues"
  },
  "homepage": "https://github.com/sanderkooger/mcp-server-ragdocs#readme",
  "dependencies": {
    "@azure/openai": "2.0.0",
    "@modelcontextprotocol/sdk": "1.0.3",
    "@qdrant/js-client-rest": "1.12.0",
    "axios": "1.8.2",
    "cheerio": "1.0.0",
    "ollama": "^0.5.14",
    "openai": "4.76.2",
    "playwright": "1.49.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/node": "^22.13.9",
    "conventional-changelog-atom": "^5.0.0",
    "husky": "^9.1.7",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
